{
	"extends": ["airbnb", "plugin:node/recommended", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": ["error"],
		"no-unused-vars": "warn",
		"no-console": "off",
		"func-names": "off",
		"no-process-exit": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "off",
		"arrow-body-style": "off",
		"prefer-arrow-callback": "off",
		"no-underscore-dangle": "off"
	}
}
